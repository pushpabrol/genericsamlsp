<!DOCTYPE html>
<html lang="en">

<% include header %>

    <body>
        <div id="wrapper" class="toggled">


            <div id="page-content-wrapper">
                <div class="container-fluid">
                    <div class="row">
                        <div class="col-lg-12">
                            <h1><span class="fa fa-info"></span> SAML SP</h1>
                            <table class="table">
                                <thead class="thead-inverse">
                                    <tr>
                                        <th>Session Index</th>
                                        <th>NameID</th>
                                    </tr>
                                </thead>
                                <tbody>
                                    <tr>
                                        <td>
                                            <%= sessionIndex %>
                                        </td>
                                        <td>
                                            <%= nameid %>
                                        </td>
                                    </tr>

                                <tbody>
                            </table>

                            <table class="table">
                                <thead class="thead-inverse">
                                    <tr>
                                        <th>Attributes</th>
                                    </tr>
                                </thead>
                                <tbody>
                                    <% for (var key in attributes)  {  if (attributes.hasOwnProperty(key))  { %>
                                        <tr>
                                            <td>
                                                <%= key %>
                                            </td>
                                            <td>
                                                <%= attributes[key] %>
                                            </td>
                                        </tr>
                                        <% } }%>

                                <tbody>
                            </table>

                        </div>
                    </div>
                </div>
            </div>

            <a href="/logout/<%= nameid %>/<%= sessionIndex %>" class="btn btn-warning btn-lg">Logout</a>
        </div>

        <% include scripts %>

    </body>

</html>